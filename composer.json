{
    "name": "hakimrazalan/enginemailer-laravel",
    "description": "EngineMailer Laravel Driver",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "php-http/guzzle7-adapter": "^1.0",
        "symfony/mailer": "^6.0 || ^7.0",
        "illuminate/support": "^9.0 || ^10.0  || ^11.0 || ^12.0",
        "hakimrazalan/enginemailer-php": "^1.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HakimRazalan\\EngineMailerLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hakim Razalan",
            "email": "hakim.razalan@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "rector/rector": "^2.0"
    }
}
